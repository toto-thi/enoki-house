        "z": "29f30ea989ea8021",
        "name": "Format Alert",
        "func": "const topicParts = msg.topic ? msg.topic.split(\"/\") : [];\nconst house = topicParts.length > 1 ? topicParts[1] : \"Unknown House\"; // Handle missing topic\n\nif (!msg.payload.actions || msg.payload.actions.length === 0) {\n    return null;\n}\n\nlet message = `ðŸš¨ Alert on Enoki House ðŸ„\\n`;\nmessage += `ðŸ  House: ${house}\\n`;\nmessage += `ðŸš€ Actuator Updates:\\n\\n`;\n\nmsg.payload.actions.forEach(action => {\n    message += `(Current Value: ${action.value})\\n${\n        action.actuator.toUpperCase()} â†’ ${action.command}\\n`;\n});\n\n\nmsg.payload = {\n    chatId: \"6263441576\", \n    type: \"message\",\n    content: message\n};\n\nreturn msg;",
